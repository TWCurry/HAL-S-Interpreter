  SIMPLE: PROGRAM;
C THE OUTUPUT BELOW SHOULD OUTPUT DURING THE LOOP
  DECLARE LOOPOUTPUT STRING DONE;
  DECLARE IEQUALS5 = I_EQUALS_5;
  DECLARE VAR SCALAR 2;
  DO FOR I = 1 TO 10;
  WRITE(1) I**VAR;
  IF I = 5;
  WRITE(1) IEQUALS5;
  ELSE;
  END;
  END;
  WRITE(1) LOOPOUTPUT;
  CLOSE SIMPLE;